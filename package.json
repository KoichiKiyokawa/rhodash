{
  "name": "rhodash",
  "version": "1.6.0",
  "description": "A deadly simple alternative to lodash",
  "types": "dist/index.d.ts",
  "main": "dist/index.cjs.js",
  "module": "dist/index.es.js",
  "browser": "dist/index.iife.js",
  "sideEffects": false,
  "files": [
    "package.json",
    "dist",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/KoichiKiyokawa/rhodash"
  },
  "scripts": {
    "prepare": "husky install",
    "build": "vite build && tsc && zx scripts/write_banner.ts",
    "lint": "prettier --write . --ignore-path .gitignore && eslint --fix . --ignore-path .gitignore",
    "lint:ci": "prettier --check . --ignore-path .gitignore && eslint . --ignore-path .gitignore",
    "test": "jest --coverage",
    "new_version": "zx scripts/new_version.ts",
    "run-ts": "ts-node -O '{\"module\":\"commonjs\"}'"
  },
  "keywords": [
    "lodash",
    "utility-library"
  ],
  "author": "K.Kiyokawa",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "27.4.0",
    "@typescript-eslint/eslint-plugin": "5.10.1",
    "@typescript-eslint/parser": "5.10.1",
    "eslint": "8.8.0",
    "eslint-config-prettier": "8.3.0",
    "husky": "7.0.4",
    "jest": "27.4.7",
    "lint-staged": "12.3.2",
    "prettier": "2.5.1",
    "ts-jest": "27.1.3",
    "ts-node": "10.4.0",
    "typescript": "4.5.5",
    "vite": "2.7.13",
    "zx": "4.3.0"
  },
  "volta": {
    "node": "16.3.0",
    "npm": "7.18.1"
  },
  "lint-staged": {
    "**/*.{ts}": [
      "eslint --fix",
      "git add"
    ],
    "**/*.{js,ts,md,json,yml}": [
      "prettier --write",
      "git add"
    ]
  }
}
