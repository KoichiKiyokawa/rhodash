{
  "name": "rhodash",
  "version": "1.5.1",
  "description": "A deadly simple alternative to lodash",
  "types": "dist/index.d.ts",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "browser": "dist/index.min.js",
  "sideEffects": false,
  "files": [
    "package.json",
    "dist",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/KoichiKiyokawa/rhodash"
  },
  "scripts": {
    "prepare": "husky install",
    "build": "rimraf dist && rollup -c",
    "lint": "prettier --write . && eslint --fix . --ignore-path .gitignore",
    "lint:ci": "prettier --check . && eslint . --ignore-path .gitignore",
    "test": "vitest --run --coverage",
    "new_version": "zx scripts/new_version.ts",
    "run-ts": "ts-node -O '{\"module\":\"commonjs\"}'"
  },
  "keywords": [
    "lodash",
    "utility-library"
  ],
  "author": "K.Kiyokawa",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-typescript": "8.3.0",
    "@typescript-eslint/eslint-plugin": "5.10.1",
    "@typescript-eslint/parser": "5.10.1",
    "c8": "7.11.0",
    "eslint": "8.5.0",
    "eslint-config-prettier": "8.3.0",
    "husky": "7.0.4",
    "lint-staged": "12.1.7",
    "prettier": "2.5.1",
    "rimraf": "3.0.2",
    "rollup": "2.61.1",
    "rollup-plugin-terser": "7.0.2",
    "ts-node": "10.4.0",
    "typescript": "4.5.5",
    "vite": "2.7.13",
    "vitest": "0.2.0",
    "zx": "4.3.0"
  },
  "volta": {
    "node": "16.3.0"
  },
  "packageManager": "pnpm@6.28.0",
  "lint-staged": {
    "**/*.{ts}": [
      "eslint --fix",
      "git add"
    ],
    "**/*.{js,ts,md,json,yml}": [
      "prettier --write",
      "git add"
    ]
  }
}
